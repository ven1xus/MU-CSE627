   "source": [
    "fig = plt.figure(1)\n",
    "\n",
    "sinX = np.linspace(0,1,100)\n",
    "sinY = np.sin(2*np.pi*sinX)\n",
    "\n",
    "x = np.linspace(0,1,10)\n",
    "y = np.sin(2*np.pi*x)\n",
    "\n",
    "beta = 10\n",
    "error = norm.rvs(scale=1./beta, size=len(y))\n",
    "t = y + error\n",
    "\n",
    "plt.plot(sinX, sinY, color='green', zorder=1)\n",
    "plt.scatter(x, t, facecolor='white', edgecolors='blue', zorder=2)\n",
    "p = Polynomial.fit(x, t, 9)\n",
    "plt.plot(*p.linspace(), color='red')"
   ]
